{
  "hosting": [{
    "public": ".next",
    "site": "documentation-main",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "next"
      }
    ]
  }, {
      "public": ".next",
      "site": "documentation-staging",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "next"
        }
      ]
    }],
  "functions": {
    "source": "functions"
  }
}