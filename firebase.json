{
  "hosting": [
    {
      "target": "documentation-main",
      "public": ".next",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/**",
          "function": "nextApp"
        }
      ]
    },
    {
      "target": "documentation-staging",
      "public": ".next",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/**",
          "function": "nextApp"
        }
      ]
    }
  ],
  "functions": {
    "source": "functions",
    "region": "us-west1"
  }
}
